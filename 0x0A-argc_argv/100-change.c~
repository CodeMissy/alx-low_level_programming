#include <stdio.h>
#include <stdlib.h>
#include "main.h"

int main(int argc, char *argv[])
{
int cent;
if (argc != 1)
printf("Error\n");
return (1);

cents++
if (atoi(argv[]) < 0)
printf("0\n");

if ((cent - 25) >= 0)
{
cent = cent - 25;
continue;
}

if ((cent - 10) >= 0)
{
cent = cent - 10;
continue;
}

if ((cent - 5) >= 0)
{
cent = cent - 5;
continue;
}

if ((cent - 2) >= 0)
{
cent = cent - 2;
continue;
}
cents--
return (0);
}
